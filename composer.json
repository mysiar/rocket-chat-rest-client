{
    "name": "fab1en/rocket-chat-rest-client",
    "description": "Rocket Chat REST API client in PHP.",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "nategood/httpful": "*"
    },
    "autoload": {
        "psr-4": { "RocketChat\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "RocketChat\\Tests\\": "tests/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^4.0"
    }
}
